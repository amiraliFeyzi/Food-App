<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/title_informatio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="monospace"
        android:text="The Fastest In Delivery Food"
        android:textColor="@color/black"
        android:textSize="16dp"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/description_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="jfsljfsklfjaskf"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        android:layout_marginTop="65dp"
        app:layout_constraintEnd_toEndOf="@+id/title_informatio"
        app:layout_constraintStart_toStartOf="@+id/title_informatio"
        app:layout_constraintTop_toBottomOf="@+id/title_informatio" />


</androidx.constraintlayout.widget.ConstraintLayout>